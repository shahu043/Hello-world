   sudo apt purge git 					#Use to remove the git installed package.
   git
   sudo apt install git
   git
   git --versio
   git --version
   git config --global user.name "shahu043" 		#Used to give the user name for your new git acccount
   git config --global user.email "shahu043@gmail.com"	#Used to give the user E-mail for your new git account
   git config --list					#Check the user details
   sudo vi .gitconfig 					#Used to change the basic information of your user account
   cd .ssh
   ll
   xclip id_rsa.pub 					#Copy data from the file
   sudo apt install xclip
